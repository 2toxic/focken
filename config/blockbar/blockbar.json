{
    "background": "#132020ff",
    "font": "Iosevka Nerd Font Light 24",
    "shortlabels": false,
    "height": 50,
    "position": "bottom",
    "trayiconsize": 1,
    "traydiv": false,
    "left": [
        {
            "mode": "subblocks",
            "exec": "~/.config/bspwm/mar0",
            "nodiv": false,
            "eachmon": true,
            "paddingleft": -2
        }
    ],
    "right": [
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/battery",
            "interval": 120000,
            "paddingright": 20
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/time",
            "interval": 31000,
            "nodiv": true,
            "paddingright": 40
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/date",
            "interval": 31000,
            "nodiv": true,
            "paddingright": 20
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/keyboard",
            "nodiv": true,
            "paddingright": 40
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/vol",
            "nodiv": true,
            "paddingright": 40
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/vpn",
            "nodiv": true,
            "interval": 60000,
            "paddingright": 40
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/jupyter",
            "nodiv": true,
            "interval": 120000,
            "paddingright": 40
        } 
    ]
}
