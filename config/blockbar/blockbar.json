{
    "background": "#132020ff",
    "font": "Inconsolata 28",
    "shortlabels": false,
    "height": 50,
    "position": "bottom",
    "trayiconsize": 1,
    "traydiv": false,
    "left": [
        {
            "mode": "subblocks",
            "exec": "~/.config/blockbar/workspaces",
            "nodiv": false,
            "eachmon": true
        }
    ],
    "right": [
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/battery",
            "interval": 120000,
            "paddingright": 20
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/time",
            "interval": 31000,
            "nodiv": true,
            "paddingright": 80
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/date",
            "interval": 31000,
            "nodiv": true,
            "paddingright": 20
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/keyboard",
            "nodiv": true,
            "paddingright": 80
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/vol",
            "nodiv": true,
            "paddingright": 80
        },
        {
            "mode": "legacy",
            "exec": "~/.config/blockbar/vpn",
            "nodiv": true,
            "paddingright": 80
        }
        
    ]
}
